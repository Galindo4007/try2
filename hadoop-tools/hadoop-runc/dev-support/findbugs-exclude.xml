<FindBugsFilter>
    <!-- Parent class is serializable which is not needed for this case -->
    <Match>
      <Class name="org.apache.hadoop.runc.docker.auth.BearerScheme" />
      <Bug pattern="SE_BAD_FIELD" />
    </Match>
    <!-- SquashFS API is meant to be low-level and avoids copying -->
    <Match>
      <Package name="org.apache.hadoop.runc.squashfs.data" />
      <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2" />
    </Match>
    <Match>
      <Package name="org.apache.hadoop.runc.squashfs.directory" />
      <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2" />
    </Match>
    <Match>
      <Package name="org.apache.hadoop.runc.squashfs.inode" />
      <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2" />
    </Match>
    <Match>
      <Package name="org.apache.hadoop.runc.squashfs.metadata" />
      <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2" />
    </Match>
    <Match>
      <Package name="org.apache.hadoop.runc.squashfs.table" />
      <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2" />
    </Match>
</FindBugsFilter>
